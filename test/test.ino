String command;

/*********************************************************************************************
	Setup
**********************************************************************************************/
void setup()
{
	Serial.begin(9600);
}

/*********************************************************************************************
	Main
**********************************************************************************************/
void loop()
{
	
	command = "S" + String(50);
	
	Serial.println(command);
	delay(5000);
	Serial.println("S");
	delay(5000);
}




