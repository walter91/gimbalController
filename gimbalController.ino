#include "RMCS2206.h"

/*********************************************************************************************
	Initialize System Objects
**********************************************************************************************/
RMCS2206 azimuth('A', 1);
RMCS2206 elevation ('E', 2);


/*********************************************************************************************
	Setup
**********************************************************************************************/
void setup()
{
	Serial.begin(9600);
	azimuth.begin();
	elevation.begin();
}

/*********************************************************************************************
	Main
**********************************************************************************************/
void loop()
{
	azimuth.move_relative(180.0);
	delay(5000);
	azimuth.move_relative(-180.0);
	delay(5000);
	
	Serial.println("S5");
	delay(5000);
	
	
}




